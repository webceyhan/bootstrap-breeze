{
    "name": "webceyhan/bootstrap-breeze",
    "description": "Minimal Laravel authentication scaffolding with Vue, Inertia & Bootstrap 5.",
    "keywords": ["laravel", "auth", "bootstrap"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/mralston/bootstrap-breeze/issues",
        "source": "https://github.com/mralston/bootstrap-breeze"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Matt Ralston",
            "email": "matt@mralston.co.uk"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/console": "^9.21|^10.0",
        "illuminate/filesystem": "^9.21|^10.0",
        "illuminate/support": "^9.21|^10.0",
        "illuminate/validation": "^9.21|^10.0"
    },
    "conflict": {
        "laravel/breeze": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Breeze\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Breeze\\BootstrapBreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
